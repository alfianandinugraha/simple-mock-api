{
  "products": [
    "{{repeat(20)}}",
    {
      "id": "{{index(1)}}",
      "name": "{{lorem(integer(1,3),'words')}}",
      "description": "{{lorem(integer(1,5))}}",
      "price": "{{floating(10000,100000,2)}}",
      "discount": "{{integer(10,30)}}"
    }
  ],
  "todos": [
    "{{repeat(20)}}",
    {
      "id": "{{index(1)}}",
      "title": "{{lorem(integer(1,5),'words')}}",
      "isDone": "{{bool()}}"
    }
  ],
  "users": [
    "{{repeat(20)}}",
    {
      "id": "{{index(1)}}",
      "name": "{{firstName() + ' ' + surname()}}",
      "phone": "{{phone()}}",
      "email": "{{email()}}",
      "company": "{{company()}}",
      "age": "{{integer(20, 30)}}"
    }
  ],
  "": {
    "message": "json-server was running, now visit /api/todos, /api/products, or /api/users"
  }
}